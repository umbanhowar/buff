{
  "manifest_version": 2,

  "name": "Buff",
  "description": "Delay delivery of email",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://apis.google.com/"
  ],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "background": {
    "scripts": [
    ]
  }
}
